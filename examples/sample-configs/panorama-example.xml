<?xml version="1.0"?>
<config version="6.0.0" urldb="brightcloud">
  <mgt-config>
    <devices/>
    <users>
      <entry name="admin">
        <phash>fnRL/G5lXVMug</phash>
        <permissions>
          <role-based>
            <superuser>yes</superuser>
          </role-based>
        </permissions>
      </entry>
    </users>
  </mgt-config>
  <shared>
    <address>
      <entry name="shared-host1">
        <ip-netmask>1.2.2.3/32</ip-netmask>
        <tag>
          <member>grp.shared-group1</member>
        </tag>
      </entry>
      <entry name="shared-host2">
        <ip-netmask>14.5.6.7/32</ip-netmask>
        <tag>
          <member>grp.shared-group1</member>
        </tag>
      </entry>
      <entry name="Datacenter Subnet">
        <ip-netmask>10.0.0.0/16</ip-netmask>
      </entry>
      <entry name="Webserver1-128.0.1.20">
        <ip-netmask>128.0.1.20</ip-netmask>
      </entry>
    </address>
    <address-group>
      <entry name="shared-group1">
        <static>
          <member>shared-host1</member>
          <member>shared-host2</member>
          <member>test-group2</member>
        </static>
      </entry>
      <entry name="test-dynamic">
        <dynamic>
          <filter>'grp.shared-group1'</filter>
        </dynamic>
      </entry>
      <entry name="test-group2">
        <static>
          <member>shared-host2</member>
        </static>
      </entry>
    </address-group>
    <tag>
      <entry name="grp.local-group1"/>
      <entry name="grp.shared-group1"/>
    </tag>
    <pre-rulebase>
      <decryption>
        <rules>
          <entry name="shared pre decrypt 1">
            <category>
              <member>any</member>
            </category>
            <type>
              <ssl-forward-proxy/>
            </type>
            <from>
              <member>any</member>
            </from>
            <to>
              <member>any</member>
            </to>
            <source>
              <member>any</member>
            </source>
            <destination>
              <member>any</member>
            </destination>
            <source-user>
              <member>any</member>
            </source-user>
            <action>no-decrypt</action>
          </entry>
        </rules>
      </decryption>
      <security>
        <rules>
          <entry name="shared pre rule 1">
            <from>
              <member>any</member>
            </from>
            <to>
              <member>any</member>
            </to>
            <source>
              <member>any</member>
            </source>
            <destination>
              <member>any</member>
            </destination>
            <source-user>
              <member>any</member>
            </source-user>
            <category>
              <member>any</member>
            </category>
            <application>
              <member>any</member>
            </application>
            <service>
              <member>application-default</member>
            </service>
            <hip-profiles>
              <member>any</member>
            </hip-profiles>
            <action>allow</action>
          </entry>
        </rules>
      </security>
    </pre-rulebase>
    <post-rulebase>
      <security>
        <rules>
          <entry name="shared post rule 1">
            <from>
              <member>any</member>
            </from>
            <to>
              <member>any</member>
            </to>
            <source>
              <member>any</member>
            </source>
            <destination>
              <member>any</member>
            </destination>
            <source-user>
              <member>any</member>
            </source-user>
            <category>
              <member>any</member>
            </category>
            <application>
              <member>any</member>
            </application>
            <service>
              <member>application-default</member>
            </service>
            <hip-profiles>
              <member>any</member>
            </hip-profiles>
            <action>allow</action>
          </entry>
        </rules>
      </security>
    </post-rulebase>
    <profile-group>
      <entry name="Shared Production Profile"/>
    </profile-group>
  </shared>
  <devices>
    <entry name="localhost.localdomain">
      <deviceconfig>
        <system>
          <ip-address>192.168.50.11</ip-address>
          <netmask>255.255.255.0</netmask>
          <update-server>updates.paloaltonetworks.com</update-server>
          <update-schedule>
            <threats>
              <recurring>
                <weekly>
                  <day-of-week>wednesday</day-of-week>
                  <at>01:02</at>
                  <action>download-only</action>
                </weekly>
              </recurring>
            </threats>
          </update-schedule>
          <timezone>Europe/Paris</timezone>
          <service>
            <disable-telnet>yes</disable-telnet>
            <disable-http>yes</disable-http>
          </service>
          <hostname>Panorama</hostname>
          <default-gateway>192.168.50.2</default-gateway>
          <dns-setting>
            <servers>
              <primary>8.8.8.8</primary>
            </servers>
          </dns-setting>
        </system>
      </deviceconfig>
      <device-group>
        <entry name="Perimeter-FWs">
          <devices/>
          <address>
            <entry name="local-host1">
              <ip-netmask>4.23.42.5/32</ip-netmask>
              <tag>
                <member>test.tag</member>
                <member>grp.local-group1</member>
              </tag>
            </entry>
          </address>
          <address-group>
            <entry name="local-group1">
              <static>
                <member>local-host1</member>
              </static>
            </entry>
          </address-group>
          <tag>
            <entry name="test.tag"/>
          </tag>
          <pre-rulebase>
            <security>
              <rules>
                <entry name="perimeter - allow users outside">
                  <from>
                    <member>internal</member>
                  </from>
                  <to>
                    <member>external</member>
                  </to>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>known-user</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>application-default</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                </entry>
              </rules>
            </security>
            <decryption>
              <rules>
                <entry name="dv1-pre decrypt 1">
                  <category>
                    <member>any</member>
                  </category>
                  <type>
                    <ssl-forward-proxy/>
                  </type>
                  <from>
                    <member>any</member>
                  </from>
                  <to>
                    <member>any</member>
                  </to>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <action>no-decrypt</action>
                </entry>
              </rules>
            </decryption>
          </pre-rulebase>
          <post-rulebase>
            <security>
              <rules>
                <entry name="no traffic out of dc">
                  <from>
                    <member>internal</member>
                  </from>
                  <to>
                    <member>any</member>
                  </to>
                  <source>
                    <member>Datacenter Subnet</member>
                  </source>
                  <destination>
                    <member>any</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>application-default</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>deny</action>
                </entry>
              </rules>
            </security>
          </post-rulebase>
        </entry>
        <entry name="Datacenter-Firewalls">
          <devices/>
        </entry>
        <entry name="ResolvZoneTest">
          <devices/>
          <pre-rulebase>
            <security>
              <rules>
                <entry name="Webfarm access">
                  <from>
                    <member>any</member>
                  </from>
                  <to>
                    <member>Public-DMZ</member>
                  </to>
                  <source>
                    <member>any</member>
                  </source>
                  <destination>
                    <member>Webserver1-128.0.1.20</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>application-default</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                </entry>
                <entry name="Access to Domain Controllers">
                  <from>
                    <member>WAN</member>
                  </from>
                  <to>
                    <member>LAN</member>
                  </to>
                  <source>
                    <member>ASIA-Offices-172.17.0.0-16</member>
                    <member>EMEA-Offices-172.18.0.0-16</member>
                  </source>
                  <destination>
                    <member>DomainController1-10.1.10.21</member>
                    <member>DomainController2-10.1.10.22</member>
                  </destination>
                  <source-user>
                    <member>any</member>
                  </source-user>
                  <category>
                    <member>any</member>
                  </category>
                  <application>
                    <member>any</member>
                  </application>
                  <service>
                    <member>application-default</member>
                  </service>
                  <hip-profiles>
                    <member>any</member>
                  </hip-profiles>
                  <action>allow</action>
                </entry>
              </rules>
            </security>
          </pre-rulebase>
          <address>
            <entry name="ASIA-Offices-172.17.0.0-16">
              <ip-netmask>172.17.0.0/16</ip-netmask>
            </entry>
            <entry name="EMEA-Offices-172.18.0.0-16">
              <ip-netmask>172.18.0.0/16</ip-netmask>
            </entry>
            <entry name="DomainController1-10.1.10.21">
              <ip-netmask>10.1.10.21</ip-netmask>
            </entry>
            <entry name="DomainController2-10.1.10.22">
              <ip-netmask>10.1.10.22</ip-netmask>
            </entry>
          </address>
        </entry>
      </device-group>
      <template>
        <entry name="ZoneResolvTest">
          <settings>
            <multi-vsys>yes</multi-vsys>
            <operational-mode>normal</operational-mode>
            <vpn-disable-mode>no</vpn-disable-mode>
          </settings>
          <config>
            <devices>
              <entry name="localhost.localdomain">
                <vsys>
                  <entry name="vsys1">
                    <zone>
                      <entry name="Internet">
                        <network>
                          <layer3>
                            <member>ethernet1/1</member>
                          </layer3>
                        </network>
                      </entry>
                      <entry name="LAN">
                        <network>
                          <layer3>
                            <member>ethernet1/2</member>
                          </layer3>
                        </network>
                      </entry>
                      <entry name="WAN">
                        <network>
                          <layer3>
                            <member>ethernet1/3</member>
                          </layer3>
                        </network>
                      </entry>
                      <entry name="Public-DMZ">
                        <network>
                          <layer3>
                            <member>ethernet1/4</member>
                          </layer3>
                        </network>
                      </entry>
                    </zone>
                    <import>
                      <network>
                        <interface>
                          <member>ethernet1/1</member>
                          <member>ethernet1/2</member>
                          <member>ethernet1/3</member>
                          <member>ethernet1/4</member>
                        </interface>
                      </network>
                    </import>
                  </entry>
                  <entry name="vsys2"/>
                </vsys>
                <network>
                  <virtual-router>
                    <entry name="vr1">
                      <protocol>
                        <bgp>
                          <routing-options>
                            <graceful-restart>
                              <enable>yes</enable>
                            </graceful-restart>
                            <as-format>2-byte</as-format>
                          </routing-options>
                          <enable>no</enable>
                        </bgp>
                      </protocol>
                      <routing-table>
                        <ip>
                          <static-route>
                            <entry name="default">
                              <nexthop>
                                <ip-address>81.255.255.1</ip-address>
                              </nexthop>
                              <metric>10</metric>
                              <destination>0.0.0.0/0</destination>
                            </entry>
                            <entry name="wan">
                              <nexthop>
                                <ip-address>192.168.1.1</ip-address>
                              </nexthop>
                              <metric>10</metric>
                              <destination>172.16.0.0/12</destination>
                            </entry>
                            <entry name="LAN-DC">
                              <nexthop>
                                <ip-address>192.168.0.1</ip-address>
                              </nexthop>
                              <metric>10</metric>
                              <destination>10.0.0.0/8</destination>
                            </entry>
                          </static-route>
                        </ip>
                      </routing-table>
                      <interface>
                        <member>ethernet1/1</member>
                        <member>ethernet1/2</member>
                        <member>ethernet1/3</member>
                        <member>ethernet1/4</member>
                      </interface>
                    </entry>
                  </virtual-router>
                  <interface>
                    <ethernet>
                      <entry name="ethernet1/1">
                        <layer3>
                          <ipv6>
                            <neighbor-discovery>
                              <router-advertisement>
                                <enable>no</enable>
                              </router-advertisement>
                            </neighbor-discovery>
                          </ipv6>
                          <ip>
                            <entry name="81.255.255.10/24"/>
                          </ip>
                        </layer3>
                      </entry>
                      <entry name="ethernet1/2">
                        <layer3>
                          <ipv6>
                            <neighbor-discovery>
                              <router-advertisement>
                                <enable>no</enable>
                              </router-advertisement>
                            </neighbor-discovery>
                          </ipv6>
                          <ip>
                            <entry name="192.168.0.10/24"/>
                          </ip>
                        </layer3>
                      </entry>
                      <entry name="ethernet1/3">
                        <layer3>
                          <ipv6>
                            <neighbor-discovery>
                              <router-advertisement>
                                <enable>no</enable>
                              </router-advertisement>
                            </neighbor-discovery>
                          </ipv6>
                          <ip>
                            <entry name="192.168.1.10/24"/>
                          </ip>
                        </layer3>
                      </entry>
                      <entry name="ethernet1/4">
                        <layer3>
                          <ipv6>
                            <neighbor-discovery>
                              <router-advertisement>
                                <enable>no</enable>
                              </router-advertisement>
                            </neighbor-discovery>
                          </ipv6>
                          <ip>
                            <entry name="128.0.1.0/24"/>
                          </ip>
                        </layer3>
                      </entry>
                    </ethernet>
                  </interface>
                </network>
              </entry>
            </devices>
          </config>
        </entry>
      </template>
    </entry>
  </devices>
</config>
